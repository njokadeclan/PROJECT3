pipeline{

    agent any

    stages{

        stage('Continuous Download') {

            steps{
                git branch: 'main', url: 'https://github.com/njokadeclan/PROJECT3.git'
            }
        }
        stage('Unit Test'){

            steps{
                sh'mvn test'
            }
        }stage('Integration Test'){

            steps{
                sh 'mvn verify -P integration-tests'
            }
        }
    }
}