{
    "config": "royale",
    "compilerOptions": {
        "debug": false,
        "targets": ["JSRoyale"],
        "source-path": [
            "src/main/resources"
        ],
        "source-map": true,
        "output": "target/javascript/",
        "html-template": "src/main/resources/plc4x-index-template.html",
        "theme": "${royalelib}/themes/JewelTheme/src/main/resources/defaults.css"
    },
    "copySourcePathAssets": true,
    "additionalOptions": "-js-dynamic-access-unknown-members=true",
    "files":
    [
        "src/main/royale/App.mxml"
    ]
}
